{
  "name": "eleven-countries",
  "version": "1.0.0",
  "description": "An 11ty multi language skeleton, powered by TailwindCSS, Webpack and Light-Server",
  "scripts": {
    "clean:d": "rm -rf ../development/*",
    "clean:p": "rm -rf ../production/*",
    "dryrun:d": "NODE_ENV=development eleventy --config=eleventy.config.js --dryrun",
    "dryrun:p": "NODE_ENV=production eleventy --config=eleventy.config.js --dryrun",
    "webpack": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "NODE_ENV=development eleventy --config=eleventy.config.js --dryrun & webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "NODE_ENV=development eleventy --config=eleventy.config.js --serve & webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build": "NODE_ENV=development eleventy --config=eleventy.config.js & webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "serve": "NODE_ENV=development light-server -c lightserver.config.json",
    "deploy": "rm -rf ../production/* && NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && NODE_ENV=production eleventy --config=eleventy.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arslabora-xi/eleven-countries.git"
  },
  "author": "Andrea Spada",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^5.0.1",
    "laravel-mix-purgecss": "^4.2.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "tailwindcss": "^1.1.2",
    "tailwindcss-dir": "^4.0.0",
    "tailwindcss-filters": "^2.0.0",
    "tailwindcss-transitions": "^2.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "light-server": "^2.6.4",
    "node-sass": "^4.12.0",
    "nunjucks-markdown": "^2.0.1"
  }
}